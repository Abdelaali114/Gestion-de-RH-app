<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/back"
    android:layout_height="match_parent"
    tools:context=".Home">
    <LinearLayout
        android:layout_width="150dp"
        android:layout_height="wrap_content"/>
        <ImageView
            android:id="@+id/menu"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="30dp"
            android:background="@drawable/menu"/>
        <ImageView
            android:id="@+id/logo1"
            android:layout_width="80dp"
            android:layout_height="70dp"
            android:layout_marginLeft="150dp"
            android:layout_marginTop="30dp"
            android:background="@drawable/logo1"/>
        <ImageView
            android:id="@+id/logout"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginLeft="320dp"

            android:layout_marginTop="30dp"

            android:background="@drawable/logout1"/>

    <LinearLayout
        android:layout_width="322dp"
        android:layout_height="428dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="168dp"
        android:layout_marginEnd="35dp"
        android:layout_marginBottom="135dp"
        android:gravity="center"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/layout1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:orientation="horizontal">


            <ImageView
                android:id="@+id/image5"
                android:layout_width="150dp"
                android:layout_height="215dp"
                android:layout_marginEnd="3dp"
                android:background="@drawable/red"></ImageView>

            <ImageView
                android:id="@+id/image6"
                android:layout_width="150dp"
                android:layout_height="215dp"
                android:layout_marginEnd="3dp"
                android:background="@drawable/bleu"></ImageView>


        </LinearLayout>

        <LinearLayout
            android:id="@+id/layout2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:orientation="horizontal">


            <ImageView
                android:id="@+id/image7"
                android:layout_width="150dp"
                android:layout_height="212dp"
                android:layout_marginEnd="3dp"
                android:background="@drawable/move"></ImageView>

            <ImageView
                android:id="@+id/image8"
                android:layout_width="150dp"
                android:layout_height="match_parent"
                android:layout_marginEnd="3dp"
                android:background="@drawable/green"></ImageView>


        </LinearLayout>


    </LinearLayout>

</RelativeLayout>